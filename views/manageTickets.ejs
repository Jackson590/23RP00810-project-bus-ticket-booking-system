<!DOCTYPE html>
<html lang="en">
<head>
    <title>Manage Tickets</title>
</head>
<body>
    <h2>Manage Tickets</h2>
    <table>
        <thead>
            <tr>
                <th>Booking ID</th>
                <th>User Name</th>
                <th>Bus Name</th>
                <th>Seat Number</th>
                <th>Booking Date</th>
            </tr>
        </thead>
        <tbody>
            <% tickets.forEach(ticket => { %>
                <tr>
                    <td><%= ticket.id %></td>
                    <td><%= ticket.user_name %></td>
                    <td><%= ticket.bus_name %></td>
                    <td><%= ticket.seat_number %></td>
                    <td><%= ticket.booking_date %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
